<!-- Add to Cart Function -->
<script>
  function handleAddToCartFurniture(event, title, price, id, quantity = 1) {
    // Prevent default if it's an event
    if (event) {
      event.preventDefault();
    }
    
    // âœ… Same Event ID as CAPI
    const serverEventId = '<%= metaEventIdCart %>';
    
    // Fire AddToCart event with synchronized event ID
    fbq('track', 'AddToCart', {
      content_name: '<%= furniture.title.replace(/'/g, "\\'") %>',
      content_ids: ['<%= furniture._id %>'],
      content_type: 'product',
      value: <%= furniture.price %> * quantity,
      currency: 'DZD',
      contents: [{
        id: '<%= furniture._id %>',
        quantity: quantity,
        item_price: <%= furniture.price %>
      }]
    }, {
      eventID: serverEventId // âœ… Same ID as CAPI
    });
    
    console.log('ðŸ›’ Furniture AddToCart Event ID:', serverEventId);
    
    // If it's a form submission, let it continue after tracking
    if (event && event.target && event.target.submit) {
      setTimeout(() => {
        event.target.submit();
      }, 500);
    }
    
    return true;
  }
</script>
</div>
</div>
<% }); %>
