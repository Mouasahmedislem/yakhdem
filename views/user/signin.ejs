                      <% if (messages.length > 0) { %>

                        <div class="alert alert-danger" role="alert">
                          <%= messages %>
                        </div>
                        
                        <% } %>
                      <!-- Grid row -->

                      <!-- Body -->
                      <form action="/user/signin" method="POST">
                      <div class="md-form mb-0">
                        <i class="fas fa-user prefix dark-grey-text"></i>
                        <input type="email" class="form-control black-text" name="email" aria-describedby="emailHelp" placeholder="Enter email">
                        <label for="exampleInputEmail1">Your Email</label>
                      </div>
                  
                      <div class="md-form mb-0">
                        <i class="fas fa-lock prefix dark-grey-text"></i>
                        <input type="password" class="form-control black-text" name="password"  placeholder="Password">
                        <label for="exampleInputPassword1">Your password</label>
                      </div>

                      <div class="text-center">
                        <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                        <button type="submit"  class="btn pink-gradient btn-rounded mt-3">Sign up</button>
                        
                      </div>
                      </form>
                      <p>Need an account ?<a href="/user/signup">Signup</a></p>
                      <p>Or go <a href="/">Home</a></p>

