  <style>
.colour-grid-surround {
  padding: 3rem 1rem;
  text-align: center;
}
.colour-grid-text {
  margin-bottom: 2rem;
}
.colour-grid-text .intro-heading {
  text-transform: capitalize;
  font-family: "Playfair Display", serif;
  font-weight: 400;
  font-size: 2.5rem;
  line-height: 1.2;
  margin-bottom: 1rem;
  color: #222;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.colour-grid-text p {
  color: #555;
  font-size: 1rem;
  max-width: 500px;
  margin: 0 auto;
}
.colour-grid-container {
  display: grid;
  grid-template-columns: repeat(2, 1fr);
  gap: 1.25rem;
}
.colour-grid-item-image img {
  width: 100%;
  border-radius: 12px 12px 0 0;
  aspect-ratio: 1;
  object-fit: cover;
  transition: transform 0.4s ease;
}
.colour-grid-item:hover img {
  transform: scale(1.05);
}
.room-label {
  background: #111;
  color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-radius: 0 0 12px 12px;
  padding: 0.75rem 1rem;
  font-size: 1rem;
}
.room-label svg {
  color: #fff;
}
@media (min-width: 768px) {
  .colour-grid-container {
    grid-template-columns: repeat(4, 1fr);
  }
  .colour-grid-text .intro-heading {
    flex-direction: row;
    gap: 0.5rem;
  }
}
.logo {
  font-size: 2.5rem;
  font-weight: bold;
  letter-spacing: 2px;
}
  
