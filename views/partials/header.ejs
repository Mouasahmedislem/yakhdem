  /* === HERO SECTION STYLING (like Coat Paints) === */
.coat.hero-container {
  position: relative;
  width: 100%;
  height: 100vh;
  overflow: hidden;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
}

.video-img-container {
  position: absolute;
  inset: 0;
  z-index: 0;
}

.video-desktop video,
.video-mobile video {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

/* Mobile/Desktop visibility */
.video-desktop { display: none; }
.video-mobile { display: block; }

@media (min-width: 768px) {
  .video-desktop { display: block; }
  .video-mobile { display: none; }
}

.coat.hero-content {
  position: relative;
  z-index: 2;
  padding: 2rem;
  max-width: 700px;
}

.coat.hero-heading p {
  font-size: 3rem;
  margin: 0;
  font-weight: 700;
  text-transform: uppercase;
  line-height: 1.1;
}

.coat.hero-subheading {
  font-size: 1.2rem;
  margin-top: 1rem;
  font-weight: 300;
  color: #f2f2f2;
}

.coat.btn--secondary {
  display: inline-flex;
  align-items: center;
  gap: 0.5rem;
  background: #fff;
  color: #000;
  padding: 0.8rem 1.5rem;
  border-radius: 50px;
  font-weight: 600;
  transition: all 0.3s ease;
  text-decoration: none;
}

.coat.btn--secondary:hover {
  background: #000;
  color: #fff;
}

.overlay::before {
  content: "";
  position: absolute;
  inset: 0;
  background: rgba(0,0,0,0.3);
  z-index: 1;
}

</style>

