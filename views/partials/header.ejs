   /* Make hero auto-fit screen height on all devices */
html, body, header {
  height: auto;
  margin: 0;
  padding: 0;
}

/* Ensure hero video fills viewport height */
.coat.hero-container {
  min-height: 100vh;
}

/* Fix spacing below hero for all devices */
header + main,
.coat.hero-container + * {
  margin-top: 0 !important;
  padding-top: 0 !important;
}

    .categories, .deals {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
      gap: 20px;
    }

    .card {
      background: #fff8f0;
      border: 1px solid #f9d9a9;
      padding: 20px;
      border-radius: 12px;
      text-align: center;
    }

    .card h3 {
      margin: 10px 0;
    }

 /* === HERO VIDEO SECTION (Smooth + Responsive) === */
.coat.hero-container {
  position: relative;
  width: 100%;
  min-height: 100vh; /* full viewport height */
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  overflow: hidden;
  text-align: center;
  margin: 0;
  padding: 0;
}

/* Background video wrapper */
.video-img-container {
  position: absolute;
  inset: 0;
  z-index: 0;
  overflow: hidden;
}

/* Video responsiveness */
.video-desktop video,
.video-mobile video {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

/* Mobile/Desktop switch */
.video-desktop { display: none; }
.video-mobile { display: block; }

@media (min-width: 768px) {
  .video-desktop { display: block; }
  .video-mobile { display: none; }
}

/* Overlay effect */
.overlay::before {
  content: "";
  position: absolute;
  inset: 0;
  background: rgba(0,0,0,0.35);
  z-index: 1;
}

/* Content area */
.coat.hero-content {
  position: relative;
  z-index: 2;
  padding: 1rem;
  max-width: 700px;
}

/* Headings */
.coat.hero-heading p {
  font-size: clamp(2.2rem, 6vw, 3.8rem);
  margin: 0;
  font-weight: 700;
  text-transform: uppercase;
  line-height: 1.1;
}

/* Subtitle */
.coat.hero-subheading {
  font-size: clamp(1rem, 2.5vw, 1.3rem);
  margin-top: 1rem;
  color: #f2f2f2;
  font-weight: 300;
}

/* Button */
.coat.btn--secondary {
  display: inline-flex;
  align-items: center;
  gap: 0.5rem;
  background: #ffffff;
  color: #000000;
  padding: 0.9rem 1.8rem;
  border-radius: 999px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 0.5px;
  transition: all 0.3s ease;
  text-decoration: none;
}

.coat.btn--secondary:hover {
  background: #000;
  color: #fff;
}

/* Ensure next section touches hero with no gap */
header + main,
.coat.hero-container + * {
  margin-top: 0 !important;
  padding-top: 0 !important;
}
@media (max-width: 768px) {
  .coat.hero-container {
    min-height: 90vh; /* slightly smaller for mobile nav space */
  }

  .coat.hero-heading p {
    font-size: 2rem;
  }

  .coat.hero-subheading {
    font-size: 0.95rem;
    padding: 0 1rem;
  }
}


</style>

